 {
    "name": "DVB",
    "version": "1.0.0",
    "description": "Zeigt an, welche Linien wann fahren",
    "author": "Donemeier",
    "license": "MIT",
    "config": [
        {
            "name": "stopId",
            "type": "array",
            "of": "number",
            "min": 0,
            "default": "33000029",
            "description": "Die Haltestellen ID (z.B. 33000029), unter https://dvb.peppnet.de/stations/ zu finden",
            "autocomplete": {
                "request": {
                    "url":"https://webapi.vvo-online.de/tr/pointfinder",
                    "method":"POST",
                    "parameter": "query"
                },
                "response": {
                    "parameter": "Points",
                    "regex": "([0-9]+)\\|"
                }
            }
        }
    ]
}
